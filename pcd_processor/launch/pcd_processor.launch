<launch>
        <param name ="/use_sim_time" value="true"/>
	<node pkg="pcd_processor" type="pcd_processor" name="pcd_processor"  output="screen">
              <remap from="/velodyne_points" to="/kitti/velo/pointcloud"/>
	      <param name="inputFiltersConfig" value="$(find pcd_processor)/launch/input_filters.yaml" />
	      <param name="minOverlap" value="0.1" /> 
	      <param name="maxOverlapToMerge" value="0.99" /> 
	      <param name="minReadingPointCount" value="1000" /> 
        </node>
</launch>

